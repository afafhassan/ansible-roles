[Unit]
Description=SonarQube service
After=syslog.target network.target

[Service]
Type=simple
ExecStart=/opt/sonarqube-9.9.3.79811/bin/linux-x86-64/sonar.sh start
ExecStop=/opt/sonarqube-9.9.3.79811/bin/linux-x86-64/sonar.sh stop
User=sonar
Group=sonar
Restart=always

[Install]
WantedBy=multi-user.target
